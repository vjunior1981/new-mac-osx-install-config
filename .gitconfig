[user]
    email = vitorjr81@gmail.com
    name = Vitor Jr.
    username = vjunior1981

[push]
        default = matching
[giggle]
        main-window-maximized = false
        main-window-geometry = 1442x680+0+54
        main-window-view = FileView

[core]
	editor = subl -n -w
    whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
	excludesfile = /Users/vitorjr/.gitignore
[web]
    browser = safari
[github]
    user = vjunior1981
[merge]
        tool = vimdiff
[color]
        branch = auto
        diff = always
        interactive = auto
        status = auto
        pager = false
        ui = auto
        ui = true
[alias]
        br = !git for-each-ref --format='%(committerdate:short),%(authorname),%(refname:short)' --sort -committerdate --color=always| column -t -s ','
        lg  = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
        clone-ignore = -c http.sslVerify=false clone

[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = red bold
        new = green bold
[credential]
    helper = store
